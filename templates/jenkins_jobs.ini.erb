# Managed by Puppet

[job_builder]
ignore_cache=<%= @r_ignore_cache %>
keep_descriptions=<%= @r_keep_descriptions %>
include_path=<%= @include_path %>
recursive=<%= @r_recursive %>
exclude=<%= @exclude %>
allow_duplicates=<%= @r_allow_duplicates %>

[jenkins]
<% if @user -%>
user=<%= @user %>
<%- end -%>
<% if @password -%>
password=<%= @password %>
<% end -%>
url=<%= @jenkins_url %>
query_plugins_info=<%= @r_query_plugins_info %>
